enable-pre-post-scripts=true
lockfile=true
prefer-workspace-packages=true
publish-branch=develop
# Pins pacakges by default
save-prefix=""
# Remove all ethersproject when ethers is updated
public-hoist-pattern[]=@ethersproject/*
# Required for FE build. Remove once FE versions are updated.
public-hoist-pattern[]=*react-transition-group*
public-hoist-pattern[]=*eventemitter3*